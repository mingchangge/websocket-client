{
  "name": "websocket-client",
  "private": true,
  "version": "0.0.1",
  "author": "",
  "description": "A WebSocket client application built with Vue.js and Electron",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite --mode dev",
    "staging": "vite --mode staging",
    "prod": "vite --mode prod",
    "ncexc": "vite --mode ncexc",
    "build-win:prod": "vite build --mode prod  && MODE=prod electron-builder --win --config build/electron-builder.js",
    "build-win:staging": "vite build --mode staging  && MODE=staging electron-builder --win --config build/electron-builder.js",
    "build-win": "vite build --mode dev && MODE=dev electron-builder --win --config build/electron-builder.js",
    "build-mac": "vite build --mode dev && MODE=dev electron-builder --mac --config build/electron-builder.js",
    "test": "vitest --mode dev",
    "coverage": "vitest run --coverage --mode dev",
    "pub-mac": "vite build --mode ${MODE} && electron-builder --mac --publish=always --config build/electron-builder.js",
    "pub-win": "vite build --mode ${MODE} && electron-builder --win --x64 --publish=always --config build/electron-builder.js",
    "preview": "vite preview --mode dev",
    "prepare": "husky install",
    "lint:eslint": "eslint \"src/**/*.{vue,js,jsx}\" --fix",
    "lint:stylelint": "stylelint \"src/**/*.{vue,css,less,scss}\" --fix"
  },
  "dependencies": {
    "area-data": "^5.0.6",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "docx-preview": "^0.3.6",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "element-china-area-data": "^5.0.2",
    "ethers": "^6.11.1",
    "fhevmjs": "^0.4.0-7",
    "gm-crypto": "^0.1.12",
    "pinia": "^2.0.14",
    "v-calendar": "^2.4.1",
    "vue": "^2.7.5",
    "vue-area-linkage": "^5.1.0",
    "vue-material-design-icons": "^5.2.0",
    "vue-router": "^3.5.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@electron/notarize": "^2.1.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^1.1.3",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^1.0.2",
    "autoprefixer": "^10.4.7",
    "echarts": "^5.4.2",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "electron-builder-notarize": "^1.5.1",
    "element-ui": "^2.15.12",
    "esbuild": "^0.17.19",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "gitlog": "^4.0.8",
    "happy-dom": "^12.9.1",
    "husky": "^8.0.1",
    "js-yaml": "^4.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "minio": "^7.1.3",
    "node-fetch": "^2.7.0",
    "pdfjs-dist": "^3.7.107",
    "postcss": "^8.4.24",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "semver": "^7.5.4",
    "stylelint": "^14.9.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vitest": "^0.34.6",
    "vue-dompurify-html": "^2.6.0",
    "vue-eslint-parser": "^9.0.3"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
